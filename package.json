{
  "name": "disjoint-union",
  "version": "0.1.0",
  "description": "Michael Bradley's personal website.",
  "repository": "git@github.com:TheDocTrier/disjoint-union.git",
  "author": "Michael Bradley <thedoctrier@gmail.com>",
  "private": true,
  "scripts": {
    "clean": "yarn del _site/**",
    "build": "yarn clean && yarn css && yarn eleventy",
    "serve": "yarn build --serve --port=1313",
    "css": "yarn postcss src/style.css -o _site/style.css",
    "prod": "NODE_ENV=production yarn build",
    "prod-serve": "yarn prod && yarn http-server _site -gba localhost -p 1313"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.11",
    "del-cli": "^4.0.1",
    "http-server": "^14.0.0",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19"
  },
  "dependencies": {}
}
